function UapiClient(c,b){this.set=function(d,e){this.data[d]=e};this.get=function(d){return this.data[d]};this.reset=function(){this.data={};this.result={};this.callbacks={onSuccess:function(d){},onError:function(d){}};this.iteratecount=0};this.getDefaultConf=function(){var d={endpoint:"signup",method:"POST"};return JSON.parse(JSON.stringify(d))};this.calculateHash=function(d){return sha1(d+this.apikey)};this.login=function(){var d=this.getDefaultConf();d.endpoint="autosignup";this.callbacks.onSuccess=this.loginSuccess.bind(this);this.callbacks.onError=this.loginError;this.callUapi(d,this.data)};this.loginError=function(d){console.log("Fail to connect to mudah user account.");console.log(d);if(d.CODE=="ERROR_USER_ACCOUNT_EMAIL_IS_PRONIAGA"){window.location.assign("/useraccount.html?error=2")}};this.loginSuccess=function(h,f){var e=f.useraccount.homepage;var d="";if(!getCookie(f.cookie.name)){setCookieByDomain(f.cookie.name,h.token,f.cookie.ttl,"/",f.cookie.domain);if(h.avatar!=""){setCookieByDomain("ua_avatar",h.avatar,f.cookie.ttl,"/",f.cookie.domain)}console.log(f.cookie.domain);var g=getParameterByName("ref");if(typeof g!="undefined"){if(g=="ai"){e=f.useraccount.insertad}}}if(this.getUrlParameter("signin")==1&&this.getUrlParameter("list_id")!=""&&this.getUrlParameter("view")==1){var d=window.location.href.replace("signin","chatroom")}else{if(this.getUrlParameter("goto_page")=="vi"){var d=window.location.origin+"/vi/"+this.getUrlParameter("list_id")+".htm"}else{if(redirect.goto_page=="vi"){var d=window.location.origin+"/vi/"+redirect.list_id+".htm"}else{var d=e}}}window.location=d};this.callUapi=function(d,f){var e=this.baseurl+e;if(typeof $=="undefined"){$=jQuery}$.post(this.baseurl+"autosignup",JSON.stringify(this.data),this.handleResponse.bind(this)).fail(function(g){var k=g.responseJSON;if(k.CODE=="ERROR_USER_ACCOUNT_INVALID_HASH"&&this.iteratecount==0){this.iteratecount++;var h=this.calculateHash(k.CHALLENGE);this.set("hash",h);setCookieByDomain(b.hash.name,h,b.cookie.ttl,"/");this.login()}else{if(k.CODE=="ERROR_VALIDATOR"){this.validatorHandler(k.ERROR)}}}.bind(this))};this.validatorHandler=function(d){redirWithRerequest(2)};this.handleResponse=function(d){if(d.STATUS=="OK"){this.callbacks.onSuccess(d,this.settings)}else{if(d.STATUS=="ERROR"){if(d.CODE=="ERROR_USER_ACCOUNT_INVALID_HASH"&&this.iteratecount==0){this.iteratecount++;var e=this.calculateHash(d.CHALLENGE);this.set("hash",e);setCookie(b.hash.name,e,b.cookie.ttl,"/");this.login()}this.callbacks.onError(d)}else{throw new Error("Whoops!")}}};this.getHash=function(){if(getCookie(this.settings.hash.name)==null){return this.settings.hash._default}else{return getCookie(this.settings.hash.name)}};this.getUrlParameter=function a(d){var g=decodeURIComponent(window.location.search.substring(1)),f=g.split("&"),h,e;for(e=0;e<f.length;e++){h=f[e].split("=");if(h[0]===d){return h[1]===undefined?true:h[1]}}};this.baseurl=c+"/api/user/";this.settings=b;this.apikey="d40053b5678a49426796818291adf96c89e4aace";this.data={};this.result={};this.conf=this.getDefaultConf();this.callbacks={onSuccess:function(d){},onError:function(d){}};this.iteratecount=0}
