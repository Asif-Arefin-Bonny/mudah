{return}c.style.display="block";var g=this.estimate()/1000;var d=this.speed();var h=this.procent();if(c.childNodes.length){var l=c.getElementsByTagName("div")[0];var e=c.getElementsByTagName("div")[1];e.style.width=Math.round((l.offsetWidth-2)*h/100)+"px";var b=c.getElementsByTagName("div")[2];var a=Math.floor(g/60);var k=Math.round(g-a*60);var f="";if(a+k>0){f="Tid kvar: "}if(a>0){f+=a+" min "}if(k>0){f+=k+" s"}b.innerHTML=h+"%&nbsp;&nbsp;&nbsp;"+f}};ProgressBar.prototype.update_draw=function(a,b){if(this.completed){return}this.update(a,b);this.draw()};function progressBar(a){document.write('<div id="loading" class="progressBar">'+a+'<span id="loading_dots"></span></div>')}function startProgressBar(d){window.onbeforeunload=null;var c=document.getElementById("loading");var b="";d%=4;for(var a=0;a<d;a++){b+="."}document.getElementById("loading_dots").innerHTML=b;d++;c.timer=setTimeout("startProgressBar("+d+")",500)}function showProgressBar(a,b){var c=document.getElementById("loading");if(!b){b=0}startProgressBar(1);c.style.top=""+(findPosY(a,true)+b)+"px";c.style.left=""+(findPosX(a,true)+20)+"px";c.style.display="inline"}function showProgressBarStatic(a,b){var c=document.getElementById("loading");if(!b){b=0}startProgressBar(1);c.style.top=""+(findPosY(a,true)+b)+"px";c.style.left="200px";c.style.display="inline"}function hideProgressBar(){var a=document.getElementById("loading");clearTimeout(a.timer);a.style.display="none";showElement("TipBox",true)}function select_all_equip(a,c,b){jQuery('form[name="'+c+'"] input[name^="'+a+'"]').prop("checked",b)}function select_all(a,c,b){jQuery('form[name="'+c+'"] input[name^="'+a+'"]').prop("checked",b)}function select_all_weeks(_name,_form,_select){for(var i=1;i<53;i++){var week=eval("document."+_form+"."+_name+i);week.checked=_select}}function check_uncheck_all(form,name,index,checked){for(var i=1;i<=index;i++){var elem=eval("document."+form+"."+name+i);elem.checked=checked}}function check_limit(form,field,field_desc,id,index